<h1>This is the mongo collection</h1>

{{#each images}}

<div class="imagecon">
            <ul class="caption">
                <li>{{title}}</li>
                <li>{{details}}</li>
            </ul>
            <img src="{{image}}" alt="Pic" class="image">
</div>

{{else}}
<p>Problem Loading Gallery...</p>

{{/each}}

